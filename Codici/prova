a=zeros(3,1);
x=1;
h=0;
D=zeros(8,3);
F=zeros(8,1);
fsoft_vec=[a(1,1)*2 + a(2,1)*3 + a(3,1)*1.5]; 
for i=1:2
   a(1)=a(1)+x 
   % fsoft_vec(i+4,1)=a
    for j=1:2
       a(2)=a(2)+x 
       % fsoft_vec(j+2,1)=b
       for k=1:2
          h=h+1;
          a(3)=a(3)+x 
          %  fsoft_vec(k,1)=c
          C = nchoosek(a,3);
          D(h,:) = C
          fsoft_vec=[a(1,1)*2 + a(2,1)*3 + a(3,1)*1.5]; 
          F(h,:) = fsoft_vec
       end
    end
end
